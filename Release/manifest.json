{
    "background_page": "background.html",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Google+ Extreme Button"
    },

    "content_scripts": [
        {
            "exclude_globs": [],
            "include_globs": ["https://plus.google.com/*"],
            "js": ["gpeb.js"],
            "permission": ["http://readitlaterlist.com/*"],
            "matches": ["http://plus.google.com/*", "https://plus.google.com/*"],
            "run_at": "document_end"
        }
    ],
    "converted_from_user_script": true,
    "default_locale": "ja",
    "description": "__MSG_extensiondescription__",
    "icons": {
        "128": "icon.png"
    },
    "name": "Google+ Extreme Button",
    "update_url": "http://clients2.google.com/service/update2/crx",
    "version": "2.0"
}
